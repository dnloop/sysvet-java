<VBox fx:id="mainVBOX" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <children>
      <MenuBar prefHeight="26.0" prefWidth="788.0">
         <menus>
            <Menu text="_Archivo">
               <items>
                  <MenuItem fx:id="miNew" mnemonicParsing="false" onAction="#miNew" text="Nuevo">
